<view class="register"><van-field vue-id="1" value="{{telephone}}" placeholder="请输入账号" type="number" maxlength="{{11}}" data-event-opts="{{[['^change',[['changePhone']]]]}}" data-com-type="wx" bind:change="__e" bind:__l="__l"></van-field><van-field vue-id="2" value="{{authCode}}" maxlength="{{6}}" center clearable placeholder="请输入短信验证码" border="{{false}}" use-button-slot data-event-opts="{{[['^change',[['changeCode']]]]}}" data-com-type="wx" bind:change="__e" bind:__l="__l" vue-slots="{{['button']}}"><van-button vue-id="{{('3')+','+('2')}}" slot="button" disabled="{{!$root.g0||codeText!='发送验证码'}}" color="linear-gradient(to right, #FFA97A, #FF6F42,#FF2B00)" size="small" type="primary" data-event-opts="{{[['^click',[['getCode']]]]}}" data-com-type="wx" bind:click="__e" bind:__l="__l" vue-slots="{{['default']}}">{{codeText}}</van-button></van-field><van-field vue-id="4" value="{{password}}" type="password" placeholder="请输入密码" maxlength="{{26}}" data-event-opts="{{[['^change',[['changePassword']]]]}}" data-com-type="wx" bind:change="__e" bind:__l="__l"></van-field><van-field vue-id="5" value="{{passwordSuer}}" type="password" placeholder="请输入密码" maxlength="{{26}}" data-event-opts="{{[['^change',[['changePasswordSuer']]]]}}" data-com-type="wx" bind:change="__e" bind:__l="__l"></van-field><view class="register-in"><van-button vue-id="6" color="linear-gradient(to right, #FFA97A, #FF6F42,#FF2B00)" type="primary" disabled="{{!telephone||!authCode||!password||!passwordSuer}}" loading="{{loading}}" block data-event-opts="{{[['^click',[['register']]]]}}" data-com-type="wx" bind:click="__e" bind:__l="__l" vue-slots="{{['default']}}">注册</van-button></view></view>